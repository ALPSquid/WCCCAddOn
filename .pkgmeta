package-as: WCCCAddOn
enable-nolib-creation: no

move-folders:
    WCCCAddOn/WCCCAddOn: WCCCAddOn

ignore: # Files and directories beginning with a dot (such as .git) are automatically ignored, as is the pgkmeta file itself.
    - build.bat
    - build.py

manual-changelog:
    filename: WCCCAddOn/CHANGELOG.md
    markup-type: markdown

license-output: WCCCAddOn/LICENSE.txt